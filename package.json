{
  "name": "source-preview-postcss",
  "version": "0.1.4",
  "description": "source-preview provider for PostCSS.",
  "keywords": [
    "postcss",
    "preview",
    "source",
    "css"
  ],
  "homepage": "http://atom.io/packages/source-preview-postcss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielbayley/atom-source-preview-postcss.git"
  },
  "bugs": {
    "url": "https://github.com/danielbayley/atom-source-preview-postcss/issues"
  },
  "author": {
    "name": "danielbayley <danielbayley@me.com>"
  },
  "private": true,
  "license": "MIT",
  "engines": {
    "atom": "*"
  },
  "activationHooks": [
    "language-postcss:grammar-used",
    "language-postcss-sugarss:grammar-used"
  ],
  "main": "./lib/main",
  "providedServices": {
    "preview.provider": {
      "versions": {
        "0.1.0": "provide"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "postcss-simple-vars": "^1.2.0"
  },
  "dependencies": {
    "postcss": "^5.0.21",
    "sugarss": "^0.1.3"
  },
  "config": {
    "valid": [
      "atcss",
      "autoprefixer",
      "cq-prolyfill",
      "css-byebye",
      "css-declaration-sorter",
      "css-mqpacker",
      "cssgrace",
      "cssnano",
      "csstyle",
      "level4",
      "lost",
      "mdcss",
      "mq4-hover-shim",
      "oldie",
      "perfectionist",
      "pixrem",
      "precss",
      "rtlcss",
      "rucksack-css",
      "stylefmt",
      "stylehacks",
      "usedcss",
      "webpcss"
    ]
  },
  "coffeelintConfig": {
    "arrow_spacing": {
      "level": "error"
    },
    "colon_assignment_spacing": {
      "level": "warn",
      "spacing": {
        "left": 0,
        "right": 1
      }
    },
    "indentation": {
      "value": 1,
      "level": "warn"
    },
    "max_line_length": {
      "value": 80,
      "level": "warn",
      "limitComments": false
    },
    "no_backticks": {
      "level": "ignore"
    },
    "no_empty_functions": {
      "level": "error"
    },
    "no_empty_param_list": {
      "level": "error"
    },
    "no_interpolation_in_single_quotes": {
      "level": "error"
    },
    "no_private_function_fat_arrows": {
      "level": "error"
    },
    "no_stand_alone_at": {
      "level": "error"
    },
    "no_tabs": {
      "level": "ignore"
    },
    "no_this": {
      "level": "error"
    },
    "no_throwing_strings": {
      "level": "warn"
    },
    "no_unnecessary_double_quotes": {
      "level": "warn"
    },
    "no_unnecessary_fat_arrows": {
      "level": "error"
    },
    "prefer_english_operator": {
      "level": "warn",
      "doubleNotLevel": "ignore"
    },
    "space_operators": {
      "level": "warn"
    }
  }
}
